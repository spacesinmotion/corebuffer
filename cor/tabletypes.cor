package Scope;
version "0.0";
root_type TableC;

table TableA {
  name: string;
  d1:unique TableD;
  d2:weak TableD;
  d3:shared TableD;
  d4:shared TableD;

  init(d3);
  init(d2, d1);
}

table TableB {
  name:string;
}

table TableD {
  name:string;
  a:shared TableA;
}

table TableC {
  a:[TableA];
  b:[TableB];

  c:[unique TableA];
  d:[shared TableB];
  e:[weak TableB];

  init(c);
}
