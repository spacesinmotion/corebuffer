package Example.Shop;
version "0.1";
root_type Shop;

table Item {
  name:string;
  price:float;
}

table Customer {
  name:string;
  address:string;
}

table Order {
  items:[shared Item];
  customer:shared Customer;
}

table Shop {
  items:[shared Item];
  customers:[shared Customer];
  orders:[Order];
}
